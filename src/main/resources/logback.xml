<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <withJansi>true</withJansi>
        <encoder>
            <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} [%0.6thread] %-5level %logger{25} - %msg %ex{30}%nopex%n</pattern>
        </encoder>
    </appender>
        <logger name="au.org.biodiversity" level="DEBUG "></logger>
        <root level="INFO">
            <appender-ref ref="STDOUT" />
        </root>
</configuration>
